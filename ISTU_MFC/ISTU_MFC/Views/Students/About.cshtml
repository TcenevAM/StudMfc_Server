@model AboutModel

@{
    Layout = "_Layout";
    ViewData["Title"] = "About";
}

<h4 class="d-flex justify-content-center">О деканате</h4>

<p name="hours-of-work">@Model.Info </p>

<p>Предоставляемые услуги:</p>

<div name="servises">
    @foreach (var item in Model.Servises)
    {
        <p name="servise1">@Html.ActionLink(@item.Name, "Servise", "Students", new { servId = @item.Id }, null)</p>
    }
    
</div>